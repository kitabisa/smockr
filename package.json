{
  "name": "smockr",
  "version": "1.2.3",
  "description": "CLI tools for serve supple mock server with random fake data using Faker.js",
  "license": "ISC",
  "author": "Kitabisa Engineer <engineer@kitabisa.com>",
  "scripts": {
    "dev": "cd commands && opaline dev",
    "prebuild": "bun build ./server.ts --outdir ./out --target bun --format esm",
    "build": "cd commands && opaline build",
    "start": "bun out/server.js",
    "publish": "npm publish"
  },
  "bin": {
    "smockr": "dist/cli.js"
  },
  "files": [
    "dist/*",
    "out/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitabisa/smockr.git"
  },
  "bugs": {
    "url": "https://github.com/kitabisa/smockr/issues"
  },
  "keywords": [
    "api",
    "mock",
    "mocking",
    "cli",
    "tools",
    "server",
    "service",
    "front-end",
    "back-end"
  ],
  "dependencies": {
    "@opaline/core": "^0.5.5",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "json-schema": "^0.4.0",
    "typescript": "^5"
  }
}
